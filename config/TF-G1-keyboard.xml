<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>  
  
	<key n="114">
	<name>r</name>
		<desc>Apply rotor brake</desc>
		<binding>
			<command>nasal</command>
			<script>gyro.apply_rotor_brake()</script>
		</binding>
		<mod-up>
		<binding>
			<command>nasal</command>
			<script>gyro.release_rotor_brake()</script>
		</binding>
		</mod-up>
	</key>
</PropertyList>

